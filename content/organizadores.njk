---
layout: base.njk
title: "Organizadores"
subtitle: "Conoce a nuestros organizadores"
order: 2
extraCss:
    - "/static/css/organizadores.css"
---

<div id="organizers">
    {% for person in organizers %}
        <div class="profile">
            <div class="img-container">
                <img src="{{ person.img }}" />
            </div>
            <div class="name">{{ person.name }}</div>
            <ul>
                {% for item in person.items %}
                    <li>
                        {{ item }}
                    </li>
                {% endfor %}
            </ul>
        </div>
    {% endfor %}
</div>